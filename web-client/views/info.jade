html
  head
    title Loading...
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/css/bootstrap.min.css', integrity='sha384-/Y6pD6FV/Vv2HJnA6t+vslU6fwYXjCFtcEpHbNJ0lyAFsXTsjBbfaDjzALeQsN6M', crossorigin='anonymous')
    link(rel='stylesheet', href='gene/info.css')
  body
    input(type='hidden' id='service-root' value='#{serviceRoot}')
    // The following code is for the error modal
    #errorModal.modal.fade(tab-index='-1', role='dialog', aria-labelledby='mySmallModalLabel', aria-hidden='true')
      .modal-dialog
        .modal-content
          .modal-header
            h4.modal-title Error
            button.close(type='button', data-dismiss='modal', aria-label='Close')
              span(aria-hidden='true')  x
          .modal-body
            p#error1
            p#error2
            p#error3
          .modal-footer
            input.btn.btn-default(type='button', data-dismiss='modal', value='Close')
    // End error modal
    .container-fluid
      iframe(src='http://dondi.github.io/GRNsight/onlyheader.html', scrolling='no')
      .row
        .col-2
          iframe#iframeMenu(src='http://dondi.github.io/GRNsight/onlysidebar.html', scrolling='no')
        #mainPage.col-10
          #pageHead.row
            .jumbotron
              div
                // This is how the gene ID's will be displayed, but the URL's provided should be altered to accodomate for each specific gene
              .row
                .col
                  h1#gene-name.display-4
                .col
                  em
                    h2#gene-species.uniProtSpecies Page is loading...
              // end of row
          #navigationButtons.row
            .col
              button.btn.btn-green(data-toggle='collapse', data-target='#generalInfo') General Information
              button.btn.btn-purple(data-toggle='collapse', data-target='#dnaSequence') DNA Sequence
              button.btn.btn-brown(data-toggle='collapse', data-target='#proteinInfo') Protein Information
              button.btn.btn-turquoise(data-toggle='collapse', data-target='#regulation') Regulation
              button.btn.btn-violet(data-toggle='collapse', data-target='#interaction') Interaction
              button.btn.btn-green(data-toggle='collapse', data-target='#geneOntology') Gene Ontology
              button.btn.btn-purple(data-toggle='collapse', data-target='#sources') Sources
          .card
            #generalInfoHeading.card-header(role='tab')
              h5.mb-0
                a.green(data-toggle='collapse',  href='#generalInfo', aria-expanded='true', aria-controls='collapseOne')
                  | General Information
            #generalInfo.collapse(role='tabpanel', aria-labelledby='headingThree')
              .card-block
                dl.row
                  dt.col-sm-3 SGD ID:
                  dd.col-sm-9
                    a.sgd-link.gene-link(data-toggle='tooltip', data-placement='top', title='Saccharomyces Genome Database')
                  dt.col-sm-3 NCBI Gene ID:
                  dd.col-sm-9
                    a.ncbi-link.gene-link(data-toggle='tooltip', data-placement='top', title='National Center for Biotechnology Information')
                  dt.col-sm-3 Ensembl ID:
                  dd.col-sm-9
                    a.ensembl-link.gene-link(data-toggle='tooltip', data-placement='top', title='Ensembl')
                  dt.col-sm-3 Uniprot ID:
                  dd.col-sm-9
                    a.uniprot-link.gene-link(data-toggle='tooltip', data-placement='top', title='UniProt')
                  dt.col-sm-3 JASPAR ID:
                  dd.col-sm-9
                    a.jaspar-link.gene-link(data-toggle='tooltip', data-placement='top', title='Jaspar')
                  dt.col-sm-3 Description:
                  dd.geneDescription.col-sm-9.sgdSource
                  dt.col-sm-3 Species:
                  dd.uniProtSpecies.col-sm-9.uniprotSource
                  dt.col-sm-3 Locus Tag:
                  dd.ncbiLocusTag.col-sm-9.ncbiSource
                  dt.col-sm-3 Family:
                  dd.jasparFamily.col-sm-9.jasparSource
                  dt.col-sm-3 Class:
                  dd.jasparClass.col-sm-9.jasparSource
                  dt.col-sm-3 Chromosome Sequence:
                  dd.chromosomeSequence.ncbiSource.col-sm-9(src='')
          .row
            .col-12
              #accordion
                .card
                  #dnaSequenceHeading.card-header(role='tab')
                    h5.mb-0
                      a.purple(data-toggle='collapse',  href='#dnaSequence', aria-expanded='true', aria-controls='collapseOne')
                        | DNA Sequence
                  #dnaSequence.collapse
                    .card-block
                      p.ensemblSource.dnaSequence.codeSequence
                .card
                  #proteinInfoHeading.card-header
                    h5.mb-0
                      a.brown(data-toggle='collapse', href='#proteinInfo')
                        | Protein Information
                  #proteinInfo.collapse
                    .card-block
                      dl.row
                        dt.col-sm-3 Protein Type:
                        dd.col-sm-9.proteinType.uniprotSource
                        dt.col-sm-3 Protein Sequence:
                        dd.col-sm-9.uniprotSource.proteinSequence.codeSequence
                .card
                  #regulationHeading.card-header
                    h5.mb-0
                      a.turquoise(data-toggle='collapse', href='#regulation', aria-expanded='true', aria-controls='collapseOne')
                        | Regulation
                  #regulation.collapse
                    .card-block
                      dl.row.dl-horizontal
                        dt.col-sm-3  Regulators:
                        dd.regulators.col-sm-9.sgdSource
                        dt.col-sm-3  Targets:
                        dd.targets.col-sm-9.sgdSource
                        dt.col-sm-3  Sequence Logo:
                        dd.col-sm-9
                          img.sequenceLogo(src='')
                        dt.col-sm-3  Frequency Matrix:
                        dd.col-sm-9
                          table.frequencyMatrix.table.table-dark.table-striped
                            thead
                              tr.frequencyOfA
                                th A:
                            tbody
                              tr.frequencyOfC
                                th C:
                              tr.frequencyOfG
                                th G:
                              tr.frequencyOfT
                                th T:
                .card
                  #interactionHeading.card-header
                    h5.mb-0
                      a.purple(data-toggle='collapse', href='#interaction', aria-expanded='true', aria-controls='collapseOne')
                        | Interaction
                  #interaction.collapse
                    .card-block
                      .physicalInteractions
                        h3  Physical Interactions
                        dl.row
                          dt.col-sm-3 Affinity Capture MS:
                          dd.affinityCaptureMS.col-sm-9.sgdSource
                          // Return Just the Value
                          dt.col-sm-3 Affinity Capture RNA:
                          dd.affinityCaptureRNA.col-sm-9.sgdSource
                          dt.col-sm-3 Affinity Capture Western:
                          dd.affinityCaptureWestern.col-sm-9.sgdSource
                          dt.col-sm-3 BioChemical Activity:
                          dd.biochemicalActivity.col-sm-9.sgdSource
                          dt.col-sm-3 Colocalization:
                          dd.colocalization.col-sm-9.sgdSource
                          dt.col-sm-3 Reconstituted Complex:
                          dd.reconstitutedComplex.col-sm-9.sgdSource
                          dt.col-sm-3 Two Hybrid
                          dd.twoHybrid.col-sm-9.sgdSource
                      .geneticInteractions
                        h3  Genetic Interactions
                        dl.row.table.table-dark
                          dt.col-sm-3 Dosage Rescue:
                          dd.dosageRescue.col-sm-9.sgdSource
                          // Return Just the Value
                          dt.col-sm-3 Negative Genetic:
                          dd.negativeGenetic.col-sm-9.sgdSource
                          dt.col-sm-3 Phenotypic Enhancement:
                          dd.phenotypicEnhancement.col-sm-9.sgdSource
                          dt.col-sm-3 Phenotypic Suppression:
                          dd.phenotypicSuppression.col-sm-9.sgdSource
                          dt.col-sm-3 Synthetic Growth Defect:
                          dd.syntheticGrowthDefect.col-sm-9.sgdSource
                          dt.col-sm-3 Synthetic Haploin Sufficiency:
                          dd.syntheticHaploinsufficiency.col-sm-9.sgdSource
                          dt.col-sm-3 Synthetic Lethality:
                          dd.syntheticLethality.col-sm-9.sgdSource
                          dt.col-sm-3 Synthetic Rescue:
                          dd.syntheticRescue.col-sm-9.sgdSource
                      dl.row
                        h5.col-sm-3 Total Interactions:
                        dd.totalInteractions.sgdSource
                .card
                  #geneOntologyHeading.card-header
                    h5.mb-0
                      a.green(data-toggle='collapse', href='#geneOntology', aria-expanded='true', aria-controls='collapseOne')
                        | Gene Ontology
                  #geneOntology.collapse
                    .card-block
                      dl.row
                        dt.col-sm-3 Gene Summary:
                        dd.geneSummary.col-sm-9.sgdSource
                        dt.col-sm-3 Molecular Function:
                        dd.molecularFunction.col-sm-9
                        dt.col-sm-3 Biological Process:
                        dd.biologicalProcess.col-sm-9
                        dt.col-sm-3 Cellular Component:
                        dd.cellularComponent.col-sm-9
                .card
                  #sourcesHeading.card-header
                    h5.mb-0
                      a.violet(data-toggle='collapse', href='#sources', aria-expanded='true', aria-controls='collapseOne')
                        | Sources
                  #sources.collapse
                    .card-block
                      dl.row
                        a#sgdSource.col.sgd-link.database-link
                      dl.row
                        a#uniprotSource.col.uniprot-link.database-link
                      dl.row
                        a#ensemblSource.col.ensembl-link.database-link
                      dl.row
                        a#ncbiSource.col.ncbi-link.database-link
                      dl.row
                        a#jasparSource.col.jaspar-link.database-link
        iframe#iframeFooter(src='http://dondi.github.io/GRNsight/onlyfooter.html', scrolling='no')
        script(src='https://code.jquery.com/jquery-3.2.1.min.js', integrity='sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=', crossorigin='anonymous')
        script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js', integrity='sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4', crossorigin='anonymous')
        script(src='https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js', integrity='sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1', crossorigin='anonymous')
        script(src='gene/api.js')
        script(src='gene/info.js')
